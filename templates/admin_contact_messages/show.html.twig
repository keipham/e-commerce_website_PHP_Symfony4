{% extends 'base.html.twig' %}

{% block title %}ContactMessages{% endblock %}

{% block body %}
    <h1>ContactMessages</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact_message.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ contact_message.username }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contact_message.email }}</td>
            </tr>
            <tr>
                <th>Object</th>
                <td>{{ contact_message.object }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ contact_message.message }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ contact_message.date ? contact_message.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EmailValidation</th>
                <td>{{ contact_message.emailValidation ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ contact_message.status }}</td>
            </tr>
            <tr>
                <th>Answer</th>
                <td>{{ contact_message.AnswerToCustomer }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_contact_index') }}">back to list</a>

    <a href="{{ path('admin_contact_edit', {'id': contact_message.id}) }}">edit</a>

    {{ include('admin_contact_messages/_delete_form.html.twig') }}
{% endblock %}
